! Xclock colors.
XClock*foreground: #dee9f0
XClock*background: #0c090c
XClock*majorColor:  rgba:de/e9/f0/ff
XClock*minorColor:  rgba:de/e9/f0/ff
XClock*hourColor:   rgba:de/e9/f0/ff
XClock*minuteColor: rgba:de/e9/f0/ff
XClock*secondColor: rgba:de/e9/f0/ff

! Set depth to make transparency work.
URxvt*depth: 32


URxvt.perl-ext-common: resize-font,default,clipboard,url-select,keyboard-select

URxvt.url-select.launcher: librewolf
URxvt.url-select.underline: true

!! URxvt Appearance
URxvt.font: xft:Operator Mono Medium:size=12.4:style=Medium
URxvt.boldFont: xft:Operator Mono Bold:size=12.4:style=Bold
URxvt.italicFont: xft:Operator Mono Medium:size=12.4:style=Medium Italic
URxvt.boldItalicfont: xft:Operator Mono Bold:size=12.4:style=Bold Italic
URxvt.scrollBar: false
URxvt.letterSpace: -1
URxvt.lineSpace: 0
URxvt.geometry: 92*24
URxvt.internalBorder: 20
URxvt.cursorBlink: true
URxvt.cursorUnderline: false
URxvt.saveline: 3072
URxvt.urgentOnBell: true



