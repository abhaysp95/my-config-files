! hard contrast: *background: #1d2021
*background: #1d2021
! soft contrast: *background: #32302f
*foreground: #ebdbb2
! Black + DarkGrey
*color0:  #1d2021
*color8:  #928374
! DarkRed + Red
*color1:  #cc241d
*color9:  #fb4934
! DarkGreen + Green
*color2:  #98971a
*color10: #b8bb26
! DarkYellow + Yellow
*color3:  #d79921
*color11: #fabd2f
! DarkBlue + Blue
*color4:  #458588
*color12: #83a598
! DarkMagenta + Magenta
*color5:  #b16286
*color13: #d3869b
! DarkCyan + Cyan
*color6:  #689d6a
*color14: #8ec07c
! LightGrey + White
*color7:  #a89984
*color15: #ebdbb2

xterm*faceName: Envy Code R
xterm*faceNameDoublesize: Droid Sans Fallback
xterm*faceSize: 12

URxvt.perl-ext-common: resize-font,default,clipboard,url-select,keyboard-select

URxvt.url-select.launcher: brave
URxvt.url-select.underline: true

!! URxvt Appearance
URxvt.font: xft:Operator Mono Medium:size=12.4:style=Medium
URxvt.boldFont: xft:Operator Mono Bold:size=12.4:style=Bold
URxvt.italicFont: xft:Operator Mono Medium:size=12.4:style=Medium Italic
URxvt.boldItalicfont: xft:Operator Mono Bold:size=12.4:style=Bold Italic
URxvt.scrollBar: false
URxvt.letterSpace: -1
URxvt.lineSpace: 0
URxvt.geometry: 92*24
URxvt.internalBorder: 20
URxvt.cursorBlink: true
URxvt.cursorUnderline: false
URxvt.saveline: 3072
URxvt.urgentOnBell: true
URxvt.depth: 24
